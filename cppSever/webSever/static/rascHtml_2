        let 
        height_vertical_1 = ( (y2_parent + w2_parent) - (y1_parent + w1_parent) ) + (vw + constante_entrada),
        reta_vertical_1_pos = reta_vertical_1.element.getBoundingClientRect(),
        posiçao_vertical_1_x = w1*0.5 - reta_vertical_1_pos.width*0.5,
        posiçao_vertical_1_y = h1;

        intNegativo(height_vertical_1) ? height_vertical_1 = -1*(height_vertical_1) :
        height_vertical_1 = height_vertical_1 ; 
            
        e1.append(reta_vertical_1.element);
        fgs.S2(reta_vertical_1.element, "height", height_vertical_1 + "px");
